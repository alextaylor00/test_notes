<%= form_for [@project, @note] do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>

    <%= f.label :text %>
    <%= f.text_area :text, class: 'form-control' %>

    <%= f.submit 'Post', class: "btn btn-primary" %>
<% end %>
